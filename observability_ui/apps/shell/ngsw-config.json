{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "basic-auth": "2.0.0",
    "shell": "2.0.0",
    "@observability/core": "2.0.0",
    "@observability/translate": "2.0.0",
    "@observability/ui": "2.0.0"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "!/scripts.*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "remotes",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/remotes/**/*.(js|css)"
        ]
      }
    },
    {
      "name": "fonts",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": [
          "https://fonts.gstatic.com/**"
        ]
      }
    }
  ]
}
